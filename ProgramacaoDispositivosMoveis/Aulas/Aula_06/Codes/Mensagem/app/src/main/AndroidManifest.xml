<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.estacio.com.mensagem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".PrincipalToastDialogActivity"
            android:label="@string/app_nameToast">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PrincipalNotificationDialogActivity"
            android:label="@string/app_nameNotification">
        </activity>
        <activity android:name=".NotificacaoActivity"></activity>
        <activity android:name=".PrincipalAlertDialogActitivy"
            android:label="@string/app_nameDialog">
        </activity>
        <activity android:name=".PrincipalProgressDialogActivity"
            android:label="@string/app_nameProgress">
        </activity>
        <activity android:name=".PrincipalDatePickerDialogActivity"
            android:label="@string/app_nameDatePicker"></activity>
        <activity android:name=".PrincipalTimePickerDialogActivity"
            android:label="@string/app_nameTimePicker"></activity>
    </application>
</manifest>
